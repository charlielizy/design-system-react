{
  "name": "@charlielizy/design-system-react",
  "description": "react typescrip design system with storybook",
  "author": "Charlie Li <charlielizy@gmail.com>",
  "version": "0.0.3",
  "private": false,
  "license": "MIT",
  "repository": "",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.38.0",
    "@blueprintjs/datetime": "^3.20.2",
    "@types/classnames": "^2.2.11",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "babel-preset-react-app": "^10.0.0",
    "classnames": "^2.2.6",
    "path": "^0.12.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.32.4",
    "sass-loader": "^10.1.1",
    "styled-components": "^5.2.1",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "buildjs": "cross-env BABEL_ENV=production babel src -d dist",
    "build": "tsc --project tsconfig.build.json",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 --quiet",
    "build-storybook": "build-storybook -s public",
    "release": "auto shipit --base-branch=main"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "absoluteRuntime": false
        }
      ]
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/node-logger": "^6.1.11",
    "@storybook/react": "^6.1.11",
    "@types/styled-components": "^5.1.6",
    "babel-loader": "8.1.0",
    "babel-plugin-styled-components": "^1.12.0",
    "themeprovider-storybook": "^1.6.4"
  }
}
